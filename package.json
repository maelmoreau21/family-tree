{
  "name": "family-tree",
  "version": "1.0.0",
  "type": "module",
  "description": "Family Tree creator and viewer (forked from family-chart)",
  "keywords": [
    "datafull",
    "family-chart",
    "family tree",
    "d3"
  ],
  "homepage": "https://maelmoreau21.github.io/family-tree/",
  "license": "MIT",
  "author": "MaÃ«l Moreau <maelmoreau21@users.noreply.github.com>",
  "main": "dist/family-tree.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/family-tree.min.js",
  "module": "dist/family-tree.esm.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/family-tree.esm.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/family-tree.js"
      }
    },
    "./styles/*": "./dist/styles/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maelmoreau21/family-tree.git"
  },
  "files": [
    "dist/**/*.js",
    "dist/styles/**",
    "dist/types/**"
  ],
  "scripts": {
    "build": "node build.js",
    "dev": "vite",
    "start": "node server/index.js",
    "data:compact": "node scripts/compact-tree.mjs",
    "docs": "typedoc",
  "docs:deploy": "npm run docs && gh-pages -d docs/api -b gh-pages",
    "prepublishOnly": "node prePublish.js",
    "test": "vitest run"
  },
  "sideEffects": false,
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "express": "^4.21.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/pg": "^8.11.6",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/d3": "^7.4.3",
    "esbuild": "^0.24.0",
    "gh-pages": "^6.3.0",
    "rollup": "^4.45.1",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.13",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^2.1.5"
  }
}
